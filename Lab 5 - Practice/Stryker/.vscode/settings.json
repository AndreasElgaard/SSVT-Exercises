{
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/*.tsbuildinfo": true,
    "**/.stryker-tmp": true,
    "**/CHANGELOG.md": true,
    "**/dist": true,
    "**/.nyc_output": true,
    "**/reports": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "task.autoDetect": "off"
}
